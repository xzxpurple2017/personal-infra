{
    "run_type": "server",
    "local_addr": "{{ trojan_config.local_addr }}",
    "local_port": {{ trojan_config.local_port }},
    "remote_addr": "{{ trojan_config.remote_addr }}",
    "remote_port": {{ trojan_config.remote_port }},
    "password": [
        "{{ trojan_password }}"
    ],
    "log_level": {{ trojan_config.log_level }},
    "ssl": {
        "cert": "{{ trojan_config.cert_path }}",
        "key": "{{ trojan_config.key_path }}",
        "key_password": "",
        "cipher": "ECDHE-ECDSA-AES128-GCM-SHA256:ECDHE-RSA-AES128-GCM-SHA256:ECDHE-ECDSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-GCM-SHA384:ECDHE-ECDSA-CHACHA20-POLY1305:ECDHE-RSA-CHACHA20-POLY1305:DHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-GCM-SHA384",
